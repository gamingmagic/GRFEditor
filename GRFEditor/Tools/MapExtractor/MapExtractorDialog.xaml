<Styles:TkWindow x:Class="GRFEditor.Tools.MapExtractor.MapExtractorDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Styles="clr-namespace:TokeiLibrary.WPF.Styles;assembly=TokeiLibrary"
                 xmlns:TokeiLibrary="clr-namespace:TokeiLibrary;assembly=TokeiLibrary"
                 xmlns:ListView="clr-namespace:TokeiLibrary.WPF.Styles.ListView;assembly=TokeiLibrary" Height="300" Width="650" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="auto" />
        </Grid.RowDefinitions>
        <Grid Name="_gridMapExtractor">
            
        </Grid>
        <Grid ListView:WpfUtils.IsDraggable="True" Grid.Row="1" Height="40" Background="{DynamicResource UIDialogBackground}">
            <DockPanel HorizontalAlignment="Right" Margin="0 0 3 0">
                <Button Name="_buttonExport" Content="Export" Height="24" Margin="3" Width="100" Click="_buttonExport_Click" />
                <Button Name="_buttonExportAt" Content="Export to..." Height="24" Margin="3" Width="100" Click="_buttonExportAt_Click" />
                <Button Content="Close" Height="24" Margin="3" Width="100" Click="_buttonClose" />
            </DockPanel>
        </Grid>
    </Grid>
</Styles:TkWindow>